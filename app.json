{
  "expo": {
    "name": "konnect",
    "slug": "konnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "main": "expo-router/entry",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nelsonelaye.konnect",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "30024551-b39e-4c90-a099-914a0b440d44"
      }
    },
    "android": {
      "package": "com.nelsonelaye.konnect"
    }
  }
}
